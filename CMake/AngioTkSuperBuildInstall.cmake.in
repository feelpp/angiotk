
# ITK
set(ANGIOTK_HAS_ITK_FROM_SUPERBUILD @ANGIOTK_HAS_ITK_FROM_SUPERBUILD@)
if (ANGIOTK_HAS_ITK_FROM_SUPERBUILD)
  set(ANGIOTK_SUPERBUILD_ITK_INSTALL_PREFIX @ANGIOTK_SUPERBUILD_ITK_INSTALL_PREFIX@)
  set(ANGIOTK_SUPERBUILD_ITK_BINARY_DIR @ANGIOTK_SUPERBUILD_ITK_BINARY_DIR@)
  execute_process( COMMAND ${CMAKE_COMMAND} -DCMAKE_INSTALL_PREFIX=${ANGIOTK_SUPERBUILD_ITK_INSTALL_PREFIX} -P ${ANGIOTK_SUPERBUILD_ITK_BINARY_DIR}/cmake_install.cmake )
endif()

