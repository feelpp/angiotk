# AngioTk
set(ANGIOTK_HAS_ANGIOTK_FROM_SUPERBUILD @ANGIOTK_HAS_ANGIOTK_FROM_SUPERBUILD@)
if (ANGIOTK_HAS_ANGIOTK_FROM_SUPERBUILD)
  set(ANGIOTK_SUPERBUILD_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
  set(ANGIOTK_SUPERBUILD_ANGIOTK_BINARY_DIR @ANGIOTK_SUPERBUILD_ANGIOTK_BINARY_DIR@)
  execute_process( COMMAND ${CMAKE_COMMAND} -DCMAKE_INSTALL_PREFIX=${ANGIOTK_SUPERBUILD_INSTALL_PREFIX} -P ${ANGIOTK_SUPERBUILD_ANGIOTK_BINARY_DIR}/cmake_install.cmake )

  if (ANGIOTK_HAS_VMTK_FROM_SUPERBUILD)
    set(VMTK_EXECUTABLE_DIR ${ANGIOTK_SUPERBUILD_VMTK_INSTALL_PREFIX}/bin)
  else()
    set(VMTK_EXECUTABLE_DIR @VMTK_EXECUTABLE_DIR@)
  endif()
  if (ANGIOTK_HAS_RORPO_FROM_SUPERBUILD)
    set(RORPO_EXECUTABLE_DIR ${ANGIOTK_SUPERBUILD_RORPO_INSTALL_PREFIX}/bin)
  else()
    set(RORPO_EXECUTABLE_DIR @RORPO_EXECUTABLE_DIR@)
  endif()
  set(AngioTk_SOURCE_DIR @AngioTk_SOURCE_DIR@)
  set(AngioTk_BINARY_DIR @AngioTk_BINARY_DIR@)
  configure_file(${AngioTk_SOURCE_DIR}/Pipeline/AngioTkInstallPipelineScript.cmake.in ${AngioTk_BINARY_DIR}${CMAKE_FILES_DIRECTORY}/AngioTkInstallPipelineScript.cmake @ONLY)
  include(${AngioTk_BINARY_DIR}${CMAKE_FILES_DIRECTORY}/AngioTkInstallPipelineScript.cmake)
endif()